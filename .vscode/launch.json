    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Flutter Windows",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
              "-d",
              "windows"
            ]
          },        
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }
    ]
}